.orderStatus {
    width: 100%;
    background: #fff;
    border-top: 1rpx solid #f0f0f0;
    padding-bottom: 30rpx;
    margin-bottom: 20rpx;
}

.top {
    display: flex;
    justify-content: space-between;
    margin-top: 63rpx;
}

.btn-wrapper {
    width: 610rpx;
    height: 88rpx;
    display: flex;
    justify-content: space-between;
    margin: auto;
    margin-top: 50rpx;
    margin-bottom: 24rpx;
}

.btn-wrapper view:nth-child(2) {
    background: #E25149;
}

.top_cancel {
    width: 100%;
    background: #fff;
    height: 130rpx;
    display: flex;
    align-items: center;
}

.top_cancel .orderCancel {
    width: 52rpx;
    height: 52rpx;
    margin: 0 16rpx 0 60rpx;
}

.top_cancel .text {
    color: #565656;
    font-size: 26rpx;
    line-height: 40rpx;
}

.top_cancel image {
    width: 52rpx;
    height: 52rpx;
}

.top .finished {
    margin-right: 86rpx;
}

.top .cutLine {
    width: 100rpx;
    height: 2rpx;
    border-radius: 10rpx;
    background: #E8E8E8;
    margin: 30rpx 5rpx 0;
    align-items: center;
}

.top .img {
    display: flex;
    justify-content: center;
    align-items: center;
}

.top image {
    width: 52rpx;
    height: 52rpx;
}

.top .buyed {
    margin-left: 70rpx;
}

.top .text {
    width: 84rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #565656;
    margin-top: 28rpx;
}

.btn {
    width: 286rpx;
    height: 88rpx;
    background: #34343E;
    color: #fff;
    line-height: 88rpx;
    text-align: center;
    border-radius: 10rpx;
    font-size: 28rpx;
}

.cancel_btn {
    width: 410rpx;
    height: 88rpx;
    background: #ddd;
    color: #fff;
    margin: 50rpx auto 24rpx;
    line-height: 88rpx;
    text-align: center;
    border-radius: 10rpx;
    font-size: 28rpx;
}

.bottom {
    width: 505rpx;
    height: 80rpx;
    line-height: 40rpx;
    font-size: 24rpx;
    margin: 0 auto;
    color: #808080;
    text-align: center;
}

.product {
    width: 100%;
    height: 180rpx;
    display: flex;
    background: #fFf;
    margin-bottom: 20rpx;
}

.left .img {
    width: 120rpx;
    height: 120rpx;
    border-radius: 10rpx;
    align-items: center;
    margin-left: 30rpx;
}

.product .img image {
    width: 120rpx;
    height: 120rpx;
}

.product .left {
    display: flex;
    width: 650rpx;
    align-items: center;
}

.left .name {
    margin: 0 0 20rpx 20rpx;
    font-size: 32rpx;
    color: #565656;
}

.left .detail {
    width: 400rpx;
    overflow: hidden;
    font-size: 26rpx;
    color: #808080;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-left: 20rpx;
}

.product .arrow {
    width: 13rpx;
    height: 21rpx;
    margin: 59rpx 0 0 50rpx;
}

.product .arrow image {
    width: 13rpx;
    height: 21rpx;
}

.address {
    width: 100%;
    height: 161rpx;
    background: #fff;
}

.address_top {
    display: flex;
}

.address_top .name {
    margin: 30rpx 0 16rpx 30rpx;
    font-size: 30rpx;
    color: #565656;
}

.address_top .phone {
    margin: 30rpx 0 0 100rpx;
    font-size: 30rpx;
    color: #565656;
}

.address_top .operation {
    width: 130rpx;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
    background: url("https://img.rekoon.cn/btn_bg.png") no-repeat;
    background-size: 130rpx 46rpx;
    color: #6A6A6A;
    font-size: 24rpx;
    margin: 30rpx 0 0 177rpx;
}

.address .cutLine {
    width: 750rpx;
    height: 6rpx;
}

.address .cutLine image {
    width: 100%;
    height: 100%;
}

.address .addressInfo {
    margin-left: 30rpx;
    font-size: 26rpx;
    color: #808080;
}

.orderInfo {
    background: #fff;
    width: 750rpx;
    margin-bottom: 210rpx;
}

.orderInfo .item {
    width: 690rpx;
    height: 98rpx;
    line-height: 98rpx;
    display: flex;
    justify-content: space-between;
    margin: 0 auto;
    border-bottom: 1rpx solid #f0f0f0;
}

.orderInfo .item .text {
    font-size: 28rpx;
    color: #808080;
}

.orderInfo .item .num {
    font-size: 28rpx;
    color: #6A6A6A;
}

.home {
    width: 84rpx;
    height: 84rpx;
    border-radius: 50%;
    position: fixed;
    bottom: 100rpx;
    right: 20rpx;
}

.home image {
    width: 84rpx;
    height: 84rpx;
}

.kefu {
    width: 74rpx;
    height: 100rpx;
    position: fixed;
    right: 20rpx;
    top: 230rpx;
}

.kefu image {
    width: 74rpx;
    height: 100rpx;
}

.hasNone {
    background: #fff;
    width: 590rpx;
    height: 374rpx;
    border-radius: 10rpx;
    padding-top: 5rpx;
}

.hasNone image {
    width: 63rpx;
    height: 63rpx;
    margin: 46rpx auto;
    display: block;
}

.hasNone .text-none {
    width: 100%;
    text-align: center;
    color: #2A2A2A;
    font-size: 36rpx;
    line-height: 50rpx;
    margin: 0 auto 60rpx;
}

.hasNone .button {
    width: 370rpx;
    height: 80rpx;
    border-radius: 10rpx;
    background: #34343E;
    color: #fff;
    font-size: 28rpx;
    line-height: 80rpx;
    text-align: center;
    margin: 0 auto;
}

.close {
    width: 44rpx;
    height: 44rpx;
    margin: 0 0 20rpx 540rpx;
}

.close image {
    width: 44rpx;
    height: 44rpx;
}

.modal-wrapper {
    width: 590rpx;
    background: #fff;
    border-radius: 10rpx;
    text-align: center;
}

.modal-wrapper .text {
    font-size: 32rpx;
    color: #565656;
    line-height: 222rpx;
}

.modal-wrapper .bottoms {
    width: 100%;
    border-top: 1rpx solid #e6e6e6;
    height: 98rpx;
    display: flex;
    justify-content: space-between;
    line-height: 98rpx;
    font-size: 30rpx;
}

.modal-wrapper .bottoms view {
    width: 50%;
    height: 84rpx;
}

.modal-wrapper .bottoms view:nth-child(1) {
    color: #6a6a6a;
    border-right: 1rpx solid #E6E6E6;
}

.modal-wrapper .bottoms view:nth-child(2) {
    color: #6D84DD;
}