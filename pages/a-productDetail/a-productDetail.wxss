    background-size: 95rpx 46rpx;
    color: #fff;
    line-height: 46rpx;
    text-align: center;
    font-size: 24rpx;
    margin-left: 30rpx;
    margin-right: 24rpx;
}

.left_top .img {
    width: 44rpx;
    height: 44rpx;
    background: url("https://img.rekoon.cn/jifens/ic_rekoon.png") no-repeat;
    background-size: 44rpx 44rpx;
    vertical-align: middle;
    margin-right: 8rpx;
}

.left_bottom {
    height: 33rpx;
    color: #808080;
    font-size: 24rpx;
    line-height: 33rpx;
    margin: 20rpx 30rpx;
}

.left_top .count {
    height: 53rpx;
    line-height: 53rpx;
    text-align: center;
    font-size: 38rpx;
    color: #DD5852;
    margin-right: 30rpx;
}

.freight {
    width: 130rpx;
    height: 33rpx;
    text-align: center;
    line-height: 33rpx;
    color: #808080;
    font-size: 24rpx;
    margin-right: 30rpx;
    margin-top: 23rpx;
    margin-bottom: 20rpx;
}

.haveCount {
    text-align: right;
    height: 40rpx;
    font-size: 28rpx;
    color: #565656;
    line-height: 40rpx;
}

.productInfo {
    padding-bottom: 1rpx;
    margin: 0;
    width: 750rpx;
    background: #fff;
    margin-bottom: 20rpx;
}

.productInfo .productName {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 90vw;
    height: 88rpx;
    line-height: 88rpx;
    color: #565656;
    font-size: 30rpx;
    text-align: left;
    margin-left: 30rpx;
}

.productInfo .info {
    font-size: 26rpx;
    margin-left: 30rpx;
    color: #808080;
    margin-right: 11rpx;
