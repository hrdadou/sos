}

.messages .lists {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
}

.messages .lists_left {
    display: flex;
    align-items: center;
}

.messages .lists_left .img {
    width: 34rpx;
    height: 34rpx;
    background: red;
    border-radius: 50%;
    margin-right: 10rpx;
}

.lists_left .phone {
    color: #565656;
