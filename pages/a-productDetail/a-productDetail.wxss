}

.active .img {
    width: 10rpx;
    height: 10rpx;
    background: url("https://img.rekoon.cn/integral/ic_mark.png") no-repeat;
    background-size: 8rpx 8rpx;
    margin-right: 8rpx;
    margin-top: 3rpx;
}

.active_left {
    display: flex;
    align-items: center;
}

.active_right {
    display: flex;
    align-items: center;
}

.active .arrow {
    width: 13rpx;
    height: 21rpx;
    background: url("https://img.rekoon.cn/ic_go.png") no-repeat;
    background-size: 13rpx 21rpx;
    margin: 0 32rpx 0 54rpx;
}

.active .text {
    font-size: 24rpx;
    height: 33rpx;
