.wrapper {
    width: 100vw;
    overflow: hidden;
}

.model-content {
    width: 100%;
}

.text-none {
    text-align: center;
    font-size: 32rpx;
    color: #565656;
    justify-content: space-between;
}

.hasNone .button {
    width: 336rpx;
    height: 121rpx;
    margin: auto;
    margin-top: 50rpx;
    background: url('https://img.rekoon.cn/jifens/sign_in_btn.png') no-repeat;
    background-size: 100% 100%;
    font-size: 30rpx;
    text-align: center;
    line-height: 92rpx;
    color: #FFFFFF;
}

.adressinfos {
    font-size: 26rpx!important;
    color: #A1A1A1!important;
    width: 100%;
    margin-left: 15rpx;
}

.change-adress {
    font-size: 24rpx;
    color: #6A6A6A;
    letter-spacing: 0.61rpx;
    background: url('https://img.rekoon.cn/jifens/btn_bg.png') no-repeat;
    background-size: 100% 100%;
    width: 130rpx;
    height: 46rpx;
    text-align: center;
    line-height: 46rpx;
    margin-right: 20rpx;
}

.default {
    border: 2rpx solid #6D84DD;
    border-radius: 6rpx;
    width: 70rpx;
    height: 32rpx;
    line-height: 32rpx;
    text-align: center;
    font-size: 20rpx!important;
    color: #6D84DD!important;
}

.hasNone {
    position: relative;
    padding-top: 197rpx;
    width: 590rpx;
    height: 448rpx;
    background: url('https://img.rekoon.cn/jifen/pop_bg.png') no-repeat;
    background-size: 100% 100%;
}

.hasNone image {
    width: 44rpx;
    height: 44rpx;
    position: absolute;
    right: 0;
    top: -5rpx;
    display: block;
}

view {
    box-sizing: border-box;
}

.line-item {
    display: flex;
    height: 100rpx;
    align-items: center;
    border-bottom: 1rpx solid #f0f0f0;
    margin-left: 30rpx;
}

.line-item.params {
    min-height: 120rpx;
    height: auto;
    position: relative;
}

.line-item.params .first {
    position: absolute;
    top: 0;
    left: 0;
}

.line-item.params .right {
    position: relative;
    left: 150rpx;
    width: 530rpx;
}

.line-item.last {
}

.line-item.adress {
    justify-content: space-between;
}

.line-item.params view {
    display: inline-block;
    font-size: 28rpx;
    color: #6a6a6a;
}

.line-item.params .right {
    padding-bottom: 10rpx;
}

.line-item.params .right view {
    padding: 0 16rpx!important;
    width: auto;
    font-size: 24rpx;
    min-width: 114rpx;
    height: 54rpx;
    text-align: center;
    line-height: 54rpx;
    border: 1px solid #BCBCBC;
    border-radius: 4rpx;
    margin: 10rpx 10rpx 0 0;
    transition: all .2s;
}

.select {
    border: 1px solid #6C6C79!important;
    background: #6C6C79;
    color: #ffffff!important;
}

.pop-info {
    background: #fafafa;
    padding: 20rpx 0 81rpx 0;
}

.pop-info .info {
    font-size: 26rpx;
    color: #A1A1A1;
    width: 690rpx;
    margin: auto;
    line-height: 40rpx;
}

.pop-info .submit {
    width: 500rpx;
    height: 98rpx;
    background: #34343E;
    border-radius: 10rpx;
    text-align: center;
    line-height: 98rpx;
    font-size: 30rpx;
    color: #FFFFFF;
    margin: 50rpx 0 0 125rpx;
}

.line-item view.first {
    font-size: 28rpx;
    color: #6A6A6A;
    width: 150rpx;
    height: 100%;
    line-height: 100rpx;
}

.line-item view.sec {
    font-size: 28rpx;
    color: #DD5852;
}

.line-item view.sec view {
    border: 1rpx solid #DD5852;
    border-radius: 10rpx;
    width: 305rpx;
    height: 50rpx;
    font-size: 24rpx;
    color: #DD5852;
    line-height: 50rpx;
    text-align: center;
}

.lbt {
    background: #fff;
    position: relative;
}

.swiper {
    width: 100%;
    height: 750rpx;
    background: #fff;
}

.swiper .slide-image {
    width: 658rpx;
    height: 658rpx;
    display: block;
    overflow: hidden;
    margin: 46rpx;
    position: relative;
}

.sell-out {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: url('https://img.rekoon.cn/integra/buy_pic_soldout_big.png') no-repeat;
    background-size: 60% 60%;
    background-position: center;
}

.swiper .slide-image image {
    width: 100%;
    height: 100%;
    display: block;
}

.indicators {
    left: 50%;
    bottom: 70rpx;
    position: absolute;
    height: 6rpx;
    transform: translate3d(-50%,0,0);
}

.indicators .i {
    display: inline-block;
    width: 30rpx;
    height: 4rpx;
    margin: 0 5rpx;
    background: rgba(0,0,0,0.20);
}

.indicators .i.active {
    background: #fff;
}

.index {
    width: 76rpx;
    background: rgba(0,0,0,.22);
    background-size: 76rpx 34rpx;
    color: #fff;
    height: 34rpx;
    line-height: 30rpx;
    font-size: 28rpx;
    border-radius: 15rpx;
    text-align: center;
    margin-left: 640rpx;
    margin-top: 20rpx;
    position: absolute;
    bottom: 50rpx;
    right: 50rpx;
}

.describe {
    background-color: #fff;
    display: flex;
    justify-content: space-between;
    border-bottom: 2rpx solid #f0f0f0;
}

.describe .left_top {
    display: flex;
    align-items: center;
    margin-bottom: 20rpx;
}

.describe .right {
    min-width: 170rpx;
    margin-right: 30rpx;
}

.left_top .status {
    width: 95rpx;
    height: 46rpx;
