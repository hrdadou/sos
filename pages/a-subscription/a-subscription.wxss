.auto {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 120rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: #fff8da;
}

.imgs {
    width: 68rpx;
    height: 75rpx;
    margin: 3rpx;
}

.imgs image {
    width: 100%;
    height: 100%;
    margin-left: 50rpx;
}

.text {
    width: 80%;
    font-size: 28rpx;
    letter-spacing: 5rpx;
    color: #808080;
    margin-right: 20rpx;
}

.wrap {
    display: flex;
    flex-direction: column;
    height: 100vh;
    background: #fff;
}

.select-content {
    position: fixed;
    top: 120rpx;
    bottom: 0;
    left: 0;
    flex: 1;
    display: flex;
    width: 100%;
}

scroll-view {
    height: 100%;
}

.left {
    width: 210rpx;
    background: #f6f6f6;
}

.left-item {
    width: 100%;
    height: 150rpx;
    margin-top: -10rpx;
    border-bottom: 1rpx solid #dcdcdc;
}

.left-item.active {
    background: #fff;
}

.left-item .img-w {
    width: 97.5rpx;
    height: 97.5rpx;
    margin: auto;
    margin-top: 10rpx;
}

.left-item .img-w image {
    width: 100%;
    height: 100%;
    display: block;
}

.left-item .model-name {
    font-size: 26rpx;
    color: #62626c;
    text-align: center;
}

.right {
    flex: 1;
}

.right-line {
    height: 100rpx;
    width: 100%;
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.right-line .icon {
    width: 118rpx;
    height: 52rpx;
    text-align: center;
    line-height: 52rpx;
    border: 1px solid #6D84DD;
    color: #6D84DD;
    font-size: 21rpx;
    border-radius: 7rpx;
    margin-right: 20rpx;
}

.right-item {
    height: 100%;
    width: 86%;
    line-height: 100rpx;
    border-bottom: 1rpx solid #f5f5f5;
    overflow: hidden;
    color: #565656;
    font-size: 30rpx;
    margin-left: 20rpx;
}

.title {
    width: 100%;
    height: 120rpx;
    background: #F3F4F6;
    display: flex;
    justify-content: space-between;
    font-size: 26rpx;
    line-height: 120rpx;
}

.title image {
    width: 30rpx;
    height: 30rpx;
    margin: 45rpx 20rpx 0 20rpx;
}

.title_left {
    display: flex;
}

.title_right {
    color: #6d84dd;
    margin-right: 20rpx;
}

.title_right_none {
    margin-right: 20rpx;
    color: #565656;
}

.subscribed {
    width: 100%;
    height: auto;
}

.prompt {
    width: 100%;
    height: 120rpx;
    font-size: 26rpx;
    color: gray;
    text-align: center;
    line-height: 120rpx;
}

.qxSubscribe {
    width: 100%;
    height: 120rpx;
    font-size: 26rpx;
    color: gray;
    line-height: 120rpx;
    display: flex;
    justify-content: space-between;
}

.qxSubscribe .right-item {
    width: 50%;
    height: 100%;
}

.qxSubscribe .icon {
    width: 117rpx;
    height: 51rpx;
    text-align: center;
    line-height: 51rpx;
    border: 1px solid #A1A1A1;
    color: #A1A1A1;
    border-radius: 8rpx;
    margin: 30rpx 20rpx 0 0;
}