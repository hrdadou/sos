.wrapper {
    width: 100%;
    min-height: 100vh;
}

.nowSignBtn {
    width: 300rpx;
    height: 110rpx;
    background: url('https://img.rekoon.cn/integral/share_btn@2x.png') no-repeat;
    background-size: 100% 100%;
    color: #fff;
    line-height: 92rpx;
    text-align: center;
    font-size: 30rpx;
    margin: auto;
    margin-top: 124rpx;
    margin-bottom: 20rpx;
}

.swichs {
    width: 230rpx;
    display: flex;
    align-items: center;
    font-size: 26rpx;
    color: #6a6a6a;
}

.nowSignBtn.more {
    background: url('https://img.rekoon.cn/integra/share_btn@2x.png');
    background-size: 100% 100%;
    width: 210rpx!important;
    height: 82rpx!important;
    line-height: 69rpx!important;
    font-size: 30rpx!important;
}

.tipe {
    display: flex;
    justify-content: center;
    align-items: center;
}

.tipe view {
    font-size: 26rpx;
    color: #808080;
}

.tipe view:last-child {
    width: 35rpx;
    height: 35rpx;
    margin-left: 10rpx;
}

.tipe view:last-child image {
    display: block;
    width: 100%;
    height: 100%;
}

.tops {
    border-top: 1rpx solid #fff;
    background: #fff;
    padding-bottom: 10rpx;
}

.goods-content {
    border-top: 1px solid #fff;
    margin: auto;
    background: #fff;
    box-sizing: border-box;
    margin-top: 20rpx;
    width: 702rpx;
    padding-bottom: 20rpx;
    padding-left: 20rpx;
}

.goods-content .title {
    display: flex;
    justify-content: center;
    align-items: center;
    color: #565656;
    font-size: 30rpx;
    margin: 20rpx;
}

.goods-content .title view {
    margin: 10rpx;
}

.goods-content .title .imgs {
    width: 66rpx;
    height: 17rpx;
}

.details view {
    margin-right: 25rpx;
    font-size: 26rpx;
    color: #6a6a6a;
    text-align: right;
    line-height: 52rpx;
}

.details {
    background: url('https://img.rekoon.cn/integra/ic@2x.png') no-repeat;
    background-size: 100% 100%;
    width: 128rpx;
    height: 52rpx;
    position: absolute;
    top: 30rpx;
    left: 0;
    z-index: 999;
}

.sigin-swich {
    position: absolute;
    top: 30rpx;
    right: 0;
    height: 60rpx;
}

.swich-btn {
    width: 100rpx;
    height: 60rpx;
    margin-left: 10rpx;
    background: rgba(232,232,232,1);
    border-radius: 100rpx;
    position: relative;
    transition: all .3s;
}

.swich-btn.height {
    background: #565656;
}

.swich-btn.height .circle {
    left: 44rpx;
}

.circle {
    width: 53rpx;
    height: 53rpx;
    background: rgba(255,255,255,1);
    box-shadow: 0px 1px 4px 0px rgba(0,0,0,0.07);
    position: absolute;
    top: 5rpx;
    left: 5rpx;
    border-radius: 100rpx;
    transition: all .3s;
}

.isSigin {
    text-align: center;
    margin-top: 124rpx;
}

.isSigin view:first-child {
    font-size: 32rpx;
    margin-top: 15px;
    color: #565656;
}

.isSigin view:first-child text {
    color: #DC3535;
}

.isSigin view:nth-child(2) {
    font-size: 24rpx;
    margin-top: 15rpx;
    color: #808080;
}

.isSigin .nowSignBtn {
    margin-top: 20rpx;
}

.goods-content .title .imgs image {
    width: 100%;
    height: 100%;
    display: block;
}

.goods-item {
    padding-top: 20rpx;
    width: 316rpx;
    min-height: 50rpx;
    border: 1rpx solid #F1F1F1;
    border-radius: 8px;
    margin-top: 21rpx;
    display: inline-block;
    margin-right: 20rpx;
}

.imgwrap {
    width: 220rpx;
    height: 220rpx;
    margin: auto;
    margin-top: 22rpx;
}

.imgwrap image {
    width: 100%;
    height: 100%;
}

.goods-item .text {
    font-size: 28rpx;
    color: #565656;
    text-align: center;
    margin: 15rpx 0 10rpx 0;
}

.goods-info {
    display: flex;
    justify-content: center;
    align-items: center;
}

.goods-info view:first-child {
    width: 32rpx;
    height: 32rpx;
    border-radius: 30rpx;
    background: #ccc;
}

.goods-info view:first-child image {
    display: block;
    width: 100%;
    height: 100%;
}

.goods-info view:nth-child(2) {
    font-size: 34rpx;
    color: #F5655B;
}

.goods-info view:nth-child(3) {
    font-size: 24rpx;
    color: #A1A1A1;
}

.goods-info view {
    margin: 0 5rpx 20rpx 0;
}

.jifen-line {
    width: 100%;
    display: flex;
    align-items: center;
    margin-bottom: 20rpx;
}

.jifen-line view:first-child {
    width: 32rpx;
    height: 32rpx;
    margin: 5rpx 10rpx 5rpx 20rpx;
}

.jifen-line view:last-child {
    font-size: 26rpx;
    color: #808080;
    letter-spacing: 0;
}

.jifen-line view:first-child image {
    display: block;
    width: 100%;
    height: 100%;
}

.loads {
    width: 100%;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    font-size: 30rpx;
    color: #565656;
}

.masker {
    width: 590rpx;
    height: 570rpx;
    border-radius: 10rpx;
    background: #fff;
    padding-top: 20rpx;
}

.hasNone {
    position: relative;
}

.closeimg {
    width: 44rpx;
    height: 44rpx;
    position: absolute;
    right: 0;
    top: -80rpx;
    display: block;
}

.masker .masker_title {
    width: 100%;
    height: 42rpx;
    line-height: 42rpx;
    text-align: center;
    font-size: 30rpx;
    color: #565656;
    margin: 40rpx auto 18rpx;
}

.masker .masker_text {
    width: 468rpx;
    line-height: 46rpx;
    font-size: 26rpx;
    color: #6A6A6A;
    margin: 5rpx auto;
}